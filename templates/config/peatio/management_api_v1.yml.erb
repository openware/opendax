keychain:
  sonic:
    algorithm: RS256
    value: <%= @sonic_public_key %>

scopes:
  read_engines:
    mandatory_signers:
      - sonic
    permitted_signers:
      - sonic
  write_engines:
    mandatory_signers:
      - sonic
    permitted_signers:
      - sonic
  read_operations:
    mandatory_signers:
      - sonic
    permitted_signers:
      - sonic

jwt: {}
